{
  "buildCommand": "cd client && npm install && npm run build:skip-check",
  "outputDirectory": "client/dist", 
  "devCommand": "npm run dev",
  "installCommand": "cd client && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}